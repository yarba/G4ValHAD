<testslist>

    <tests>
        <testdes>test48</testdes>
        <g4version>geant4-10.00-p01</g4version>
        <observable>kinetic energy of secondary neutron</observable>
        <reaction>capture pi- on C</reaction>
        <beam>pi-</beam>
	<beammomentum>0.0 MeV</beammomentum>
	<beamenergy></beamenergy>
        <target>C</target>
        <secondary>neutron</secondary>
        <status>public</status>
        <score>
            <type>expert</type>
            <value>passed</value>  
        </score>
        <imageblob>pim-C-Bert-regre.gif</imageblob>
        <taglist>
            <tag>
                <name>Model</name>
                <value>Bertini</value>
            </tag>
            <tag>              
                <name>Regression testing:</name>
                <value>geant 4 versions: g4-9-6-p02,g4-9-6-p03,g4-10.00,g4-10.00-p01</value>
            </tag>             
	    <tag>
	       <name>Comment</name>
	       <value>Bertini is using PreCompound to handle evaporation</value>
	    </tag>             
        </taglist>    
    </tests>

